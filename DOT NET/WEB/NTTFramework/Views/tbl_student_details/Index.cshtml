@model IEnumerable<NTTFramework.tbl_student_details>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.studName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.studGender)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.studSubject)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.studName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.studGender)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.studSubject)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.studRollno }) |
            @Html.ActionLink("Details", "Details", new { id=item.studRollno }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.studRollno })
        </td>
    </tr>
}

</table>
